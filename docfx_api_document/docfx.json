{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Uplink2.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "api/Miniscript*.yml",
          "docs/api_name_overrides.md",
          "filterConfig.yml"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/api_name_overrides.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/uplink"
    ],
    "globalMetadata": {
      "_appName": "",
      "_appTitle": "",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
